           <schema_name/>
           </partitioning>
    <connection>conn_oracle</connection>
    <sql>SELECT m.media_id,m.channel_id,n.COPYRIGHT_TYPE_ID AS COPYRIGHT_TYPE_ID  FROM   

(

   SELECT b.media_id,c.channel_id FROM 

   (SELECT media_id,channel_name FROM dm_common_media  GROUP BY media_id,channel_name) b , dm_common_channel c 

   WHERE b.channel_name=c.channel_name(+)

) m,

(

   SELECT b.media_id,c.COPYRIGHT_TYPE_ID FROM 

  (SELECT media_id,is_copyright FROM dm_common_media  GROUP BY media_id,is_copyright) b, DM_COMMON_COPYRIGHT c 

   WHERE b.is_copyright=c.COPYRIGHT_TYPE_NAME(+)

) n

WHERE m.media_id=n.media_id(+)
</sql>
