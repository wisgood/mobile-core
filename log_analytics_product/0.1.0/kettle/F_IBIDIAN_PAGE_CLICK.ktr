    // prepare regex and field helpers
    // 如果是刚刚处理第一条记录(初始),则建立各变量(JAVA)与各字段(record)及各输入/输出表间的关联
    if (first) {
        first = false;
        // get the input and output fields
        transf_dim_index  = get(Fields.Out, "pagename_id");
        native_dim_value  = get(Fields.In, "page_name");
        // get all rows from the info stream and compile the regex field to patterns
        import_dim_index  = get(Fields.Info, "dim_index");
        import_dim_value  = get(Fields.Info, "dim_value");
        value2index_Map   = new HashMap();
        RowSet table_RowSet = findInfoRowSet("dim_table");
        Object[] dim_Row    = null;
        while((dim_Row = getRowFrom(table_RowSet)) != null) {
            String key = import_dim_value.getString(dim_Row);
            long value = import_dim_index.getInteger(dim_Row);
            value2index_Map.put(key, value);
            if(value > maxId) {
                maxId = value;
            }
        }
        export_RowSet = findTargetRowSet("export_dim_appends");
        transf_RowSet = findTargetRowSet("further_processing");
    }
    // 如果最后一条记录都处理完了就退出结束
    Object[] record = getRow(); // 获取一行记录
    if (record == null) {
        setOutputDone();
        return false;
    }
    
    // get the value of the field to check
    String   tmp_Key  = native_dim_value.getString(record);
    long     tmp_Id   = 0;
    // 创建的一条新的记录
    Object[] upRecord = createOutputRow(record, data.outputRowMeta.size());
    if (!value2index_Map.containsKey(tmp_Key)) {
        maxId++;
        // 这里必须用tmp_Id,因为最后的更新记录情况会用到tmp_Id
        tmp_Id = maxId;
        // 加入到HashMap里,以供下一条记录判断
        value2index_Map.put(tmp_Key, tmp_Id);
        // 创建一条新的记录(包含新增加的字段),并将其传递下去,而放弃老的记录:record
        transf_dim_index.setValue(upRecord, Long.valueOf(tmp_Id));
        putRowTo(data.outputRowMeta, upRecord, export_RowSet);
    }
    else {
        tmp_Id = (Long)value2index_Map.get(tmp_Key);
        transf_dim_index.setValue(upRecord, Long.valueOf(tmp_Id));
    }
    // 都会执行到这
    // Send the row on to the next step.
    putRowTo(data.outputRowMeta, upRecord, transf_RowSet);
 
    return true;
}]]></class_source>
