        <class_source><![CDATA[import java.util.HashMap;
import java.util.Iterator;
 
private FieldHelper outTag = null;
private FieldHelper outTagId = null;
private FieldHelper inputTag = null;

private RowSet TagOutStream = null;
private RowSet resultOutStream = null;

private HashMap tag2Id; // = new HashMap();
private long maxId = 0;
 
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	
    // prepare regex and field helpers
    if (first){
        first = false;

	    // get the input and output fields
        outTag = get(Fields.Out, "NEW_TAG");
		outTagId = get(Fields.Out, "TAG_ID");
        
        inputTag = get(Fields.In, "NEW_TAG");

		tag2Id=new HashMap();	

		TagOutStream = findTargetRowSet("tag_out");
		resultOutStream = findTargetRowSet("result_out");
    }
	
 	Object[] r = getRow();
 
    if (r == null) {
        setOutputDone();
		return false;
    }

    // get the value of the field to check
	String tmpTag = inputTag.getString(r);
		
	long tmpTagId = 0; 
	//Entry value;
	
    if (!tag2Id.containsKey(tmpTag)){
		maxId++;
		tmpTagId = maxId;
		tag2Id.put(tmpTag, tmpTagId);
		
		Object[] tr = createOutputRow(r, data.outputRowMeta.size());
		outTag.setValue(tr, String.valueOf(tmpTag));
		outTagId.setValue(tr, Long.valueOf(tmpTagId));
		
		putRowTo(data.outputRowMeta, tr, TagOutStream);
	} else {
		tmpTagId = (Long)tag2Id.get(tmpTag);
	}
	

    Object[] outRow = createOutputRow(r, data.outputRowMeta.size());
    outTag.setValue(outRow, String.valueOf(tmpTag));
	outTagId.setValue(outRow, Long.valueOf(tmpTagId));

    // Send the row on to the next step.
    putRowTo(data.outputRowMeta, outRow, resultOutStream);
 
    return true;
}]]></class_source>
