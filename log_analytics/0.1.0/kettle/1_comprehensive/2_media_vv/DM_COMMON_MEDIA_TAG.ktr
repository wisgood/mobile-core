    <connection>db_conn_corsair</connection>
    <sql>SELECT
	p.serialid AS SERIAL_ID,
	p.mediaid AS MEDIA_ID,
	m.tag4editor AS TAG
FROM 
	fs_web_playinfo AS p,
	fs_media AS m
WHERE
